def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up
#push_characters "kkkkkkkkkkkkkkk"
def_key "ctrl-d"
    page_down
#push_characters "jjjjjjjjjjjjjjj"

def_key "L"
    seek_forward
def_key "H"
    seek_backward

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "h"
    previous_column
def_key "l"
    next_column

# not used but bound
def_key "J"
    move_sort_order_down
def_key "K"
    move_sort_order_up
